{% extends 'base.twig' %}

{% block content %}
<div class="container">
    <div class="row" style="padding-top: 200px;">
        <div class="col-sm-3">
            {{ menu_vertical({'items': items,'item_activo': item_activo}) }}
        </div>
        <div class="col-sm-9">
            {% for r in reservas %}
                {{ r.options.template_historial|raw }}
            {% else %}
                {{ trans('error_no_se_encontraron_elementos') }}
            {% endfor %}
        </div>
    </div>
</div>
{% endblock %}